<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>TheArchiverAPI</Title>
        <Authors>pauljoda</Authors>
        <Description>Package to create download interfaces</Description>
        <Version>1.0.1</Version>
        <PackageProjectUrl>https://github.com/pauljoda/DownloadManager/tree/main</PackageProjectUrl>
        <RepositoryUrl>https://github.com/pauljoda/DownloadManager/tree/main</RepositoryUrl>
        <RepositoryType>GitHub</RepositoryType>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="HtmlAgilityPack" Version="1.11.72" />
    </ItemGroup>

</Project>
