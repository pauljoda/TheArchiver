{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "TheArchiver.Monitor": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Monitor": {
    "MaxConsoleLines": 1000,
    "MaxBufferSize": 1000,
    "RetryDelaySeconds": 2,
    "MaxRetries": 3,
    "CircuitBreakerTimeoutMinutes": 1,
    "MessageQueueProcessorIntervalSeconds": 5,
    "EnableDetailedLogging": true,
    "AllowedOrigins": [
      "http://localhost:5000",
      "https://localhost:5001"
    ]
  }
}
